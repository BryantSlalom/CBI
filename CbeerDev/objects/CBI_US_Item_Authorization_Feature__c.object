    <fields>
        <fullName>TYPE_DSC__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(ISPICKVAL(Type_CD__c, &quot;BE&quot;), RecordType.DeveloperName = &quot;Feature&quot;), &quot;Beer of the Month&quot;,
   IF(AND(ISPICKVAL(Type_CD__c, &quot;HH&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Happy Hour&quot;,
      IF(AND(ISPICKVAL(Type_CD__c, &quot;LP&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Every Day Low Price&quot;,
         IF(AND(ISPICKVAL(Type_CD__c, &quot;LT&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Limited Time Only (LTO)&quot;,
            IF(AND(ISPICKVAL(Type_CD__c, &quot;be&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Beer of the Month&quot;,
               IF(AND(ISPICKVAL(Type_CD__c, &quot;hh&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Happy Hour&quot;,
                  IF(AND(ISPICKVAL(Type_CD__c, &quot;lp&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Every Day Low Price&quot;,
                     IF(AND(ISPICKVAL(Type_CD__c, &quot;lt&quot;),RecordType.DeveloperName = &quot;Feature&quot;), &quot;Limited Time Only (LTO)&quot;,
IF(AND(ISPICKVAL(Type_CD__c, &quot;OS&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Authorized - Optional (Sell-In)&quot;,
   IF(AND(ISPICKVAL(Type_CD__c, &quot;SP&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Authorized - Select Planogram&quot;,
      IF(AND(ISPICKVAL(Type_CD__c, &quot;BM&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Brand Mandated - Any Format&quot;,
         IF(AND(ISPICKVAL(Type_CD__c, &quot;CM&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Corporate Mandate&quot;,
            IF(AND(ISPICKVAL(Type_CD__c, &quot;NA&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Not Applicable&quot;,
               IF(AND(ISPICKVAL(Type_CD__c,&quot;PA&quot;), RecordType.DeveloperName = &quot;Item_Authorization&quot;),&quot;Pending Authorization&quot;,
                  IF(AND(ISPICKVAL(Type_CD__c, &quot;PN&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Presented - Not Authorized&quot;,
                     IF(AND(ISPICKVAL(Type_CD__c, &quot;TB&quot;), RecordType.DeveloperName = &quot;Item_Authorization&quot;),&quot;To be Presented&quot;,
                        IF(AND(ISPICKVAL(Type_CD__c, &quot;os&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Authorized - Optional (Sell-In)&quot;,
                           IF(AND(ISPICKVAL(Type_CD__c, &quot;sp&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Authorized - Select Planogram&quot;,
                             IF(AND(ISPICKVAL(Type_CD__c, &quot;bm&quot;), RecordType.DeveloperName = &quot;Item_Authorization&quot;),&quot;Brand Mandated - Any Format&quot;,
                                 IF(AND(ISPICKVAL(Type_CD__c, &quot;cm&quot;), RecordType.DeveloperName = &quot;Item_Authorization&quot;),&quot;Corporate Mandate&quot;,
                                    IF(AND(ISPICKVAL(Type_CD__c, &quot;na&quot;), RecordType.DeveloperName = &quot;Item_Authorization&quot;),&quot;Not Applicable&quot;,
                                      IF(AND(ISPICKVAL(Type_CD__c, &quot;pa&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Pending Authorization&quot;,
                                               IF(AND(ISPICKVAL(Type_CD__c, &quot;pn&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;Presented - Not Authorized&quot;,
                                                       IF(AND(ISPICKVAL(Type_CD__c, &quot;tb&quot;),RecordType.DeveloperName = &quot;Item_Authorization&quot;), &quot;To be Presented&quot;, null))))))))))))))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
